
entity Project {
	title String required
}

entity Bug {
	code String required,
    state String,
    description String required,
    date String,
    dependenceList String
}

entity Solution {
	solution String required
}

relationship OneToMany {
	Project to Bug{project},
}

relationship OneToMany {
 	Bug to Solution{bug},
}


dto * with mapstruct
service * with serviceImpl



